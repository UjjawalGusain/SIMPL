func factorial(number n) {
    number result = 1;
    number current = n;

    if (current < 0) {
        return 1;
    }

    while (current > 1) {
        result = result * current;
        current = current - 1;
    }
    return result;
}

func main() {
    print("Factorial of 0:");
    number res0 = factorial(0);
    print(res0); 

    print("Factorial of 1:");
    number res1 = factorial(1);
    print(res1);

    print("Factorial of 5:");
    number res5 = factorial(5); 
    print(res5); 

    print("Factorial of 7:");
    number res7 = factorial(7);
    print(res7); 
}